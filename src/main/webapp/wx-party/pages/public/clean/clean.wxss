.container .tab-nav-wrapper{
  padding-top: 84rpx;
  width: 100%;
  z-index: 50!important;
  white-space: nowrap;
  height: 82rpx;
  background: #fff;
  border-bottom: 2rpx solid #DFDFDF;
}
.container .tab-nav-wrapper .nav-item{
  display: inline-block;
  width: 180rpx;
  height: 80rpx;
  color: #999;
  line-height: 80rpx;
  font-size: 28rpx;
  text-align: center;
  border-bottom: 3rpx solid transparent; 
}
.container .tab-nav-wrapper .nav-item.on{
  color: #6F9EED;
  border-color: #6F9EED;
}
.container .clean-list .item-right{
  height: 100%;
  position: relative;
  overflow: hidden;
   line-height: 116rpx; 
  padding: 0 25rpx 0 20rpx;
}
.container .clean-list .item-right .clean-title{
  font-size: 32rpx;
  color: #333;
  display: block;
  text-overflow: ellipsis;  
  overflow: hidden;
  white-space: nowrap;
}