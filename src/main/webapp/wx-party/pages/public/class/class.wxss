.container .class-wrapper{padding-top: 124rpx;}
.container .class-swiper-wrapper{position: relative; }
.container .class-swiper-wrapper .swiper-nav{
  position: absolute;  
  left: 0;  
  right: 0;  
  bottom: 20rpx;  
  display: flex;
  padding-right: 5rpx;
  justify-content: flex-end;
}
.container .class-swiper-wrapper .swiper-nav text{
  width: 15rpx;
  height: 15rpx;
  margin: 0 5rpx;
  border-radius: 50%;
  background-color: rgba(0, 0, 0, .5);
}
.container .class-swiper-wrapper .swiper-nav text.on{background-color: #fff;}
.container .class-swiper{height: 420rpx;}
.container .class-swiper swiper-item{
  position: relative;
  width: 100%;
  height: 100%;
  overflow: hidden;
}
.container .class-swiper swiper-item image{width: 100%;height: 100%;}
.container .class-swiper swiper-item view{
  position: absolute;
  width: 100%;
  bottom: 0;
  height: 64rpx;
  line-height: 64rpx;
  background: rgba(0, 0, 0, .3);
}
.container .class-swiper swiper-item view > text{
  display: block;
  color: #fff;
  font-size: 28rpx;
  width: 75%;
  text-indent: 20rpx;
  overflow: hidden;
  text-overflow:ellipsis;
  white-space: nowrap;
}
.container .model-title{
  position: relative;
  font-size: 32rpx;
  color: #333;
  margin-top: 30rpx;
  padding: 20rpx 0 20rpx 50rpx;
  border-top: 2rpx solid #ddd;
}
.container .model-title::after{
  content: "";
  position: absolute;
  top: 50%;
  transform: translateY(-50%);
  left: 30rpx;
  width: 8rpx;
  height: 50rpx;
  background-color: #E83C3C;
}