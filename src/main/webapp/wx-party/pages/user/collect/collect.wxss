.container .collect-item{
  padding: 25rpx 0 0 30rpx;
  border-bottom: 2rpx solid #ddd; 
}
.container .collect-item .item-img{
  float: left;
  width: 160rpx;
  height: 120rpx;
  overflow: hidden;
  padding-bottom: 25rpx;
  margin-right: 25rpx;
}
.container .collect-item .item-img image{width: 100%;height: 100%;}
.container .collect-item .item-content{
  overflow: hidden;
  padding:0 30rpx 24rpx 0;
}
.container .collect-item .item-content view{
  font-size: 28rpx;
  color: #333;
  height: 72rpx;
  line-height: 36rpx;
  display: -webkit-box;
  -webkit-box-orient: vertical;
  -webkit-line-clamp: 2;
  overflow: hidden;
}
.container .collect-item .item-content text{
  color: #999;
  font-size: 24rpx;
}